{
  "manifest_version": 3,
  "name": "HearAI",
  "version": "1.0",
  "description": "HearAI",
  "permissions": ["activeTab", "storage", "contextMenus", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://192.168.3.14:3000/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "HearAI",
    "default_icon": {
      "16": "logo_16.png",
      "48": "logo_48.png",
      "128": "logo_128.png"
    }
  },
  "icons": {
    "16": "logo_16.png",
    "32": "logo_48.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  }
}
